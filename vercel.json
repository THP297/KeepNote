{
    "version": 2,
    "builds": [
      {
        "src": "mykeep/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "mykeep/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "mykeep.settings"
    },
    "build": {
      "functions": {
        "preBuild": { "command": "pip install -r requirements.txt" },
        "postBuild": { "command": "python manage.py collectstatic --noinput" }
      }
    }
  }
  